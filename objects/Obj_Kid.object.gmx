          <argument>
            <kind>1</kind>
            <string>//Imput Vars
if global.Warp = 0
{
Hold_Left = keyboard_check(Key_Left)
Hold_Right = keyboard_check(Key_Right)
Hold_Jump = keyboard_check(Key_Jump)
Hold_Shoot = keyboard_check_pressed(Key_Shoot)
}

if global.Warp = 0
{
Press_Jump = keyboard_check_pressed(Key_Jump)
Press_Shoot = keyboard_check_pressed(Key_Shoot)
Press_Up = keyboard_check_pressed(Key_Up)
Press_Change = keyboard_check_pressed(Key_Change)
Press_Trans = keyboard_check_pressed(Key_Trans)
}
else
{
Press_Jump = 0
Press_Shoot = 0
Press_Up = 0
Press_Change = 0
Press_Trans = 0
}

//Walk
MoveDirection = -Hold_Left + Hold_Right
if MoveDirection = -1
 {if HSpeed &gt; -WalkSpeed
  HSpeed -= Acelerarion
 }

if MoveDirection = 1
 {if HSpeed &lt; WalkSpeed
  HSpeed += Acelerarion
 }

if MoveDirection = 0
{if HSpeed &lt; 0
  {if HSpeed &gt; -Acelerarion
   {HSpeed += Acelerarion}
  else
   {HSpeed = 0}}
 if HSpeed &gt; 0
  {if HSpeed &lt; Acelerarion
   {HSpeed -= Acelerarion}
  else
   {HSpeed = 0}}
}

//Gravity = 0.25
//Jump
if (place_meeting(PosX,PosY+1,Obj_Sub_Block))
|| (place_meeting(PosX,PosY+1,Obj_Platform) &amp;&amp; VSpeed &gt;= 0 &amp;&amp; !place_meeting(PosX,PosY,Obj_Platform))
{OnFloor = 1
if DJump &lt; 1
{DJump = 1}}
else
{OnFloor = 0}

if (place_meeting(PosX,PosY+VSpeed+1,Obj_Block_Parent))
|| (place_meeting(PosX,PosY+VSpeed+1,Obj_Platform) &amp;&amp; VSpeed &gt;= 0 &amp;&amp; !place_meeting(PosX,PosY,Obj_Platform))
{OnSolid = 1}
else
{OnSolid = 0}

if (place_meeting(PosX,PosY+VSpeed+1,Obj_Block_Parent))
{OnBlock = 1}
else
{OnBlock = 0}

if Press_Jump &amp;&amp; DJump &gt;= 1 &amp;&amp; !OnFloor
{DJump -= 1
VSpeed = -DJumpSpeed
audio_play_sound_on(global.SFX,Snd_DJump,false,1)}

if Press_Jump &amp;&amp; OnFloor
{VSpeed = -JumpSpeed
audio_play_sound_on(global.SFX,Snd_Jump,false,1)}

if VSpeed &lt; -MinFallSpeed &amp;&amp; !Hold_Jump
{VSpeed = -MinFallSpeed}

if VSpeed &gt; MaxFallSpeed
{VSpeed = MaxFallSpeed}

VSpeed += Gravity

//Shoot
if MoveDirection != 0
{Facing = MoveDirection}

/// Collision Code

//Horizontal Collision
if (place_meeting(PosX+HSpeed,PosY,Obj_Solid))
{
    //while(!place_meeting(PosX+sign(HSpeed),PosY,Obj_Solid))
    //{
    //    PosX += sign(HSpeed);
    //}
    HSpeed = 0;
}

PosX += HSpeed
x = round(PosX)

//Vertical Collision
if (place_meeting(PosX,PosY+VSpeed,Obj_Solid))
{
    while(!place_meeting(PosX,PosY+sign(VSpeed),Obj_Solid))
    {
        PosY += sign(VSpeed);
    }
    VSpeed = 0;
}
else
{
if (place_meeting(PosX,PosY+VSpeed+1,Obj_Platform)) &amp;&amp; VSpeed &gt;= 0 &amp;&amp; !place_meeting(PosX,PosY,Obj_Platform)
{
    while(!place_meeting(PosX,PosY+sign(VSpeed),Obj_Platform))
    {
        PosY += sign(VSpeed);
    }
    VSpeed = 0;
}}

PosY += VSpeed
y = round(PosY)
</string>
